{
  "scripts": {
    "build": "npm run scripts && npm run styles",
    "start": "PORT=3000 node server public",
    "scripts": "browserify -t [ babelify --presets [ es2015 react ] ] -t [ envify purge ] app.js > public/app.js",
    "styles": "lessc styles/app.less > public/app.css",
    "test": "xo --ignore public/*.js"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "envify": "vdemedes/envify#env-files",
    "eslint-config-vdemedes": "^1.0.0",
    "koa": "^1.1.2",
    "koa-static": "^1.5.2",
    "less": "^2.5.3",
    "xo": "^0.11.2"
  },
  "dependencies": {
    "backbone": "^1.2.3",
    "basscss": "^7.0.4",
    "clone": "^1.0.2",
    "codemirror": "^5.9.0",
    "debounce": "^1.0.0",
    "firebase": "^2.3.2",
    "jquery": "^2.1.4",
    "mapbox.js": "^2.2.3",
    "normalize.css": "^3.0.3",
    "object-assign": "^4.0.1",
    "object.omit": "^2.0.0",
    "pinkie-promise": "^2.0.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.3",
    "slugg": "^0.1.2"
  },
  "xo": {
    "extends": "vdemedes",
    "env": "browser"
  }
}
